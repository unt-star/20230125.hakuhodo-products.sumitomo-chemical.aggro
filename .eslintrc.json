{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "globals": {
    "extend": "readonly"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      "warn"
    ],
    "no-irregular-whitespace": [
      "error",
      {
        "skipTemplates": true
      }
    ],
    "no-unused-vars": [
      "warn"
    ]
  }
}
